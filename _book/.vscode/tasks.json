{
  "tasks": [
    {
      "type": "shell",
      "label": "gcc build active file",
      "command": "/usr/bin/gcc",
      "args": [
        "-g",
        "${file}",
        "-o",
        "${fileBasenameNoExtension}.out"
      ],
      "options": {
        "cwd": "/usr/bin"
      }
    },
    {
      "type": "shell",
      "label": "gcc-5 build active file",
      "command": "/usr/bin/gcc-5",
      "args": [
        "-g",
        "${file}",
        "-o",
        "${fileDirname}/${fileBasenameNoExtension}"
      ],
      "options": {
        "cwd": "/usr/bin"
      }
    }
  ],
  "version": "2.0.0"
}